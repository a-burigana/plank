(define (domain active-muddy-child)
    (:requirements :del :modal-preconditions :multi-pointed-models :knowing-whether)

    (:action-type-libraries ma-star)

    (:predicates (muddy ?i - agent))

    (:action ask
        :parameters (?i - agent)
        :action-type (public-sensing)
        :precondition
            (e1 ([Kw. ?i] (muddy ?i))
             e2 (<Kw. ?i> (muddy ?i)))
        :effects ()
        :observability-conditions (always All Fully)
    )
)
