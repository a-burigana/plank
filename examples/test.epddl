(define (domain cb)
    (:requirements :del :typing :ontic-actions (:max-modal-depth 3)
                       :modal-preconditions
    )

    (:action-type-libraries ma_star)

    (:types agent peschereccio - object Giulio - agent)

    (:predicates (tails)
                 (opened ?box)
                 (looking ?i - agent)
                 (has_key ?i)
    )

    (:action open
        :parameters (?ag - agent)
        :precondition (has_key ?ag)
        :action-type (ontic
                        (~effects (opened))
                     )
        :observability-conditions (All Fully)
    )
)
